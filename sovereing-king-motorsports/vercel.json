{
    "builds": [
      {
        "src": "sovereing-king-motorsports/wsgi.py",
        "use": "@vercel/python"
      },
      {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sovereing-king-motorsports/wsgi.py"
      }
    ]
}
